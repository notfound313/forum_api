{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest":true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-undef":"off",
        "no-console": "off",
        "no-unused-vars":"off",
        "camelcase":"off",
        "class-methods-use-this":"off"
    
    }
}
